    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "eslint": "^7.11.0",
